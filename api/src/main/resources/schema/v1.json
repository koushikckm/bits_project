{
  "type" : "object",
  "required" : ["data"],
  "properties" : {
        "data" : {
          "type" : "object",
          "required" : ["firstName", "ssn", "dateOfBirth", "address", "phoneNo", "city", "state", "pin"],
          "properties" : {
            "ssn" : {
              "type" : "string",
              "pattern" : "^[a-zA-Z0-9_-]*$"
            },
            "dateOfBirth" : {
              "type" : "string",
              "format" : "date-time"
            },
            "address" : {
              "type" : "string"
            },
            "phoneNo" : {
              "type" : "number",
              "pattern" : "^[0-9]*$"
            },
            "city" : {
              "type" : "string"
            },
            "state" : {
              "type" : "string"
            },
            "pin" : {
              "type" : "number",
              "pattern" : "^[0-9]*$"
            }
          }
        }
      }
}